<?xml version='1.0' encoding='utf-8'?>
<widget id="com.izipass.izipass" version="3.0.0.10" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="ios-package" value="CDVBarcodeScanner" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="Vibration">
        <param name="ios-package" value="CDVVibration" />
    </feature>
    <name>Fidelite IziPass</name>
    <description>
    Gestion Fidélité
  </description>
    <author email="louis.teboul@izi-pass.com" href="http://izipass.pro">
    IZIPASS
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="*" />
    <allow-navigation href="*" />
    <icon src="resources/ios/icon/icon-72@2x.png" />
    <gap:splash src="resources/ios/splash/Default@2x~iphone.png" />
    <icon gap:platform="ios" height="228" src="resources/ios/icon/Icon-App-76x76@3x.png" width="228" />
    <icon gap:platform="ios" height="152" src="resources/ios/icon/Icon-App-76x76@2x.png" width="152" />
    <icon gap:platform="ios" height="76" src="resources/ios/icon/Icon-App-76x76@1x.png" width="76" />
    <icon gap:platform="ios" height="180" src="resources/ios/icon/Icon-App-60x60@3x.png" width="180" />
    <icon gap:platform="ios" height="120" src="resources/ios/icon/Icon-App-60x60@2x.png" width="120" />
    <icon gap:platform="ios" height="60" src="resources/ios/icon/Icon-App-60x60@1x.png" width="60" />
    <icon gap:platform="ios" height="80" src="resources/ios/icon/Icon-App-40x40@2x.png" width="80" />
    <icon gap:platform="ios" height="40" src="resources/ios/icon/Icon-App-40x40@1x.png" width="40" />
    <icon gap:platform="ios" height="87" src="resources/ios/icon/Icon-App-29x29@3x.png" width="87" />
    <icon gap:platform="ios" height="58" src="resources/ios/icon/Icon-App-29x29@2x.png" width="58" />
    <icon gap:platform="ios" height="29" src="resources/ios/icon/Icon-App-29x29@1x.png" width="29" />
    <gap:splash gap:platform="ios" height="768" src="resources/ios/splash/Default-Landscape~ipad.png" width="1024" />
    <gap:splash gap:platform="ios" height="1024" src="resources/ios/splash/Default-Portrait~ipad.png" width="768" />
    <gap:splash gap:platform="ios" height="1136" src="resources/ios/splash/Default-568h@2x~iphone.png" width="640" />
    <gap:splash gap:platform="ios" height="960" src="resources/ios/splash/Default@2x~iphone.png" width="640" />
    <gap:splash gap:platform="ios" height="480" src="resources/ios/splash/Default~iphone.png" width="320" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="phonegap-version" value="cli-6.3.0" />
    <preference name="cordova-version" value="cli-6.3.0" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="KeyboardShrinksView" value="true" />
</widget>
